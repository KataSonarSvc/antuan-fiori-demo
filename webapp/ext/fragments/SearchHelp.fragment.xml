<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:core="sap.ui.core"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:layout="sap.ui.layout"
	xmlns:smartfilterbar="sap.ui.comp.smartfilterbar"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:forms="sap.ui.layout.form"
>
	<ValueHelpDialog
		title="title"
		ok="handleOnValueHelpOkPressed"
		cancel="handleOnValueHelpCancelPressed"
		afterClose="_onValueHelpAfterClose"
		key="FieldName"
		descriptionKey="FieldDescription"
		supportMultiselect="true"
	>
		<filterBar>
			<fb:FilterBar
				advancedMode="false"
				search=".handleFilterBarSearch"
				isRunningInValueHelpDialog="true"
				enableBasicSearch="true"
			>
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="FieldDependent1"
						label="FieldDependent1"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input
								name="FieldDependent1"
								editable="false"
							/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="FieldDependent2"
						label="FieldDependent2"
						visible="false"
						visibleInFilterBar="false"
					>
						<fb:control>
							<m:Input
								name="FieldDependent2"
								editable="false"
							/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="AdditionalField3"
						label="AdditionalField3"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input
								name="AdditionalField3"
								editable="true"
								liveChange="handleLiveChangeFGI"
							/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="AdditionalField4"
						label="AdditionalField4"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input
								name="AdditionalField4"
								editable="true"
								liveChange="handleLiveChangeFGI"
							/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="AdditionalField5"
						label="AdditionalField5"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input
								name="AdditionalField5"
								editable="true"
								liveChange="handleLiveChangeFGI"
							/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="AdditionalField6"
						label="AdditionalField6"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input
								name="AdditionalField6"
								editable="true"
								liveChange="handleLiveChangeFGI"
							/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="AdditionalField7"
						label="AdditionalField7"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input
								name="AdditionalField7"
								editable="true"
								liveChange="handleLiveChangeFGI"
							/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="AdditionalField8"
						label="AdditionalField8"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input
								name="AdditionalField8"
								editable="true"
								liveChange="handleLiveChangeFGI"
							/>
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>